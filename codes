<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <title>Conditional statements</title>
</head>
<body>
    <script>
        let age = 34;
        if(age>18){
            console.log("You can drive");
        }
        else if(age==2){
            console.log("Age is 2");

        }
        else{
            console.log("You cannot drive");
        }

    </script>
</body>
