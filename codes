    var string1 = "This is a string1";
    var string1 = "This is a string2";
    console.log(string1);
    let a = "u";
    {
     let a = "u6";
     console.log(a);
    }
    console.log(a);

    const b = "This cannot be changed";
    console.log(b);
   </script>
